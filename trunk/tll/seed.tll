void screenSetup() {
//   grid No, step in UU, color
   definegrid(0, .25 , "white"   );
   definegrid(1, 2   , "cyan"    );
   definegrid(2, 10  , "yellow"  );
   grid(0,true);
   grid(1,true);
   step(0.5);
   autopan(true);
   shapeangle(45);
}

void bug1() 
{
   newcell("bug1");
   opencell("bug1");
   addbox( { { 2 , 7 } , { 18 , 17.5 } }, 2);
   select({8.50,8.50});
   rotate({ 2 , 7 },30);
   select({3.50,8.00});
   addpoly( { { 1.5 , 5.5 } , { 4 , 5.5 } , { 4 , 7 } , { 2 , 7 } , { 1.5 , 7 } }, 4);
