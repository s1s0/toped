
//demonstrates the easy of antenna design

#include "config.tll"
#include "default_drc.tll"
#include "helper.tll"
#include "geometry.tll"
#include "wires.tll"

newdesign("via_test");
newcell("via");
opencell("via");

//start of the  design

point list a = { {0,0}, {100,100}, {200,100} };
point list b = { {50,0}, {130,70}, {200,70} };

addwire_on_grid (a, 30, true);
addwire_on_grid (b, 20, false);


point list c;
real width = 30;
real dist = width + 10;
point center = { 0,0};
point dia = { 300, 300 };

foreach (int t ; seq(1,10)) {

    c[:+] = { center.x + dia.x + (t-1)*dist, center.y + dia.y + (t-1)*dist };
    c[:+] = { center.x + dia.x + (t-1)*dist, center.y - dia.y - (t-1)*dist };
    c[:+] = { center.x - dia.x - (t-1)*dist, center.y - dia.y - (t-1)*dist };
    c[:+] = { center.x - dia.x - (t-1)*dist, center.y + dia.y + (t)*dist };
    
}

addwire_on_grid (c, width, true);

//end of design

