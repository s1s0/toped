#include "config/config.tll"

void layerSetup() {
  lmap list cifmap;
  lmap list gdsmap;
  
  //source: http://www.mosis.com/Technical/Designrules/scmos/scmos-main.html
  
  //    	     name    Number  Colour       Fill              Selected

  layprop("POLY"     ,46   , "lime green"   ,"checkerboard"   , "selected3" );cifmap[:+] = {{ 46, "CPG" }}; gdsmap[:+] = {{ 46, " 46;  0"}};
  layprop("POLY2"    ,56   , "dark green"   ,"checkerboard"   , "selected3" );cifmap[:+] = {{ 56, "CEL" }}; gdsmap[:+] = {{ 56, " 56;  0"}};
