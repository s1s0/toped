  layprop("MET2"     ,51   , "red"          ,"checkerboard"   , "selected3" );cifmap[:+] = {{ 51, "CM2" }}; gdsmap[:+] = {{ 51, " 51;  0"}};
  layprop("MET3"     ,62   , "yellow"       ,"checkerboard"   , "selected3" );cifmap[:+] = {{ 62, "CM3" }}; gdsmap[:+] = {{ 62, " 61;  0"}};
  layprop("MET4"     ,31   , "magenta"      ,"checkerboard"   , "selected3" );cifmap[:+] = {{ 31, "CM4" }}; gdsmap[:+] = {{ 31, " 31;  0"}};
